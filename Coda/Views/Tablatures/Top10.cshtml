<h2>Top 10</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Song.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Content)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Song.Name)
        </td>
        <td>
            @Html.ActionLink("Tab", "Details", "Tablatures", new {id=item.Id})
        </td>
    </tr>
}

</table>
