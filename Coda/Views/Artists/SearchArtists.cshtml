@model IEnumerable<Coda.Models.Artist>

@{
    int songId = 0; 
}

<table class="table">
   
    @foreach (var item in Model)
    {
        foreach (var song in item.Songs)
        {

            songId = song.ArtistId;

        }
        @Html.Action("Index", "Songs", new {id=songId})

    }
</table>
